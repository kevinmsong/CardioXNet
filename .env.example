# CardioXNet Configuration Example
# Copy this file to .env and adjust values as needed

# Application Settings
NETS_APP_NAME=CardioXNet
NETS_ENVIRONMENT=development
NETS_DEBUG=true
NETS_LOG_LEVEL=INFO

# Server Configuration
NETS_HOST=0.0.0.0
NETS_PORT=8000

# CORS Settings
NETS_CORS_ORIGINS=["http://localhost:3000","http://localhost:8000"]

# Paths
NETS_DATA_DIR=data
NETS_OUTPUT_DIR=outputs
NETS_CONFIG_DIR=config

# API Configuration
NETS_STRING_VERSION=11.5
NETS_GPROFILER_VERSION=e104_eg51_p15
NETS_REACTOME_VERSION=79

# Analysis Parameters
NETS_STRING_SCORE_THRESHOLD=0.7
NETS_FDR_THRESHOLD=0.05
NETS_TOP_HYPOTHESES_COUNT=5

# Database Weights
NETS_DB_WEIGHT_REACTOME=1.5
NETS_DB_WEIGHT_KEGG=1.5
NETS_DB_WEIGHT_WIKIPATHWAYS=1.2
NETS_DB_WEIGHT_GOBP=1.0

# API Endpoints (optional - defaults provided)
# NETS_STRING_API_URL=https://string-db.org/api
# NETS_GPROFILER_API_URL=https://biit.cs.ut.ee/gprofiler/api
# NETS_REACTOME_API_URL=https://reactome.org/ContentService

# Retry Configuration
NETS_MAX_RETRIES=3
NETS_RETRY_BACKOFF_FACTOR=2.0
NETS_REQUEST_TIMEOUT=30

# Aggregation Configuration
NETS_AGGREGATION_STRATEGY=intersection
NETS_MIN_SUPPORT_THRESHOLD=2

# Literature Mining Configuration
NETS_PUBMED_MAX_RESULTS=50
NETS_LITERATURE_RELEVANCE_THRESHOLD=0.3

# Output Configuration
NETS_OUTPUT_FORMATS=["markdown","json","html"]
